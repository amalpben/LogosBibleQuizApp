    <style name="GenQuizCardLayout">
        <item name="android:layout_width">210dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/bg_semiblack_card</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingVertical">28dp</item>
        <item name="android:paddingHorizontal">16dp</item>
        <item name="android:layout_marginVertical">32dp</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="GenQuizCardLayout2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bg_white_all_round</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginBottom">8dp</item>
    </style>
    <style name="GenQuizCardLayout2Img">
        <item name="android:layout_width">32dp</item>
        <item name="android:layout_height">32dp</item>
    </style>
    <style name="GenQuizCardLayout2Sub">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="GenQuizCardLayout2Txt">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/aerospace_bold</item>
        <item name="android:paddingHorizontal">8dp</item>
        <item name="android:textColor">@color/secondary</item>
        <item name="android:textSize">16dp</item>
    </style>
    <style name="GenQuizCardLayoutTxt">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18dp</item>
        <item name="android:fontFamily">@font/aerospace_bold</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:textAlignment">center</item>
    </style>
    <style name="GenQuizCardLayoutTxtSyl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/secondary</item>
        <item name="android:textSize">16dp</item>
